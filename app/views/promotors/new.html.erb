<%= form_for @promotor do |f| %>
  <%= f.hidden_field :user_id, :value => current_user.id %>
  <%= f.label "City" %>
  <%= f.text_field :location, :placeholder => "New York City" %> <br>
  <%= f.label "Email" %>
  <%= f.text_field :email %>
  <%= f.submit "Create Promotor Account" %>
<% end %>

 <script>
     $("h1.header-title").html("New Promotor");
 </script>